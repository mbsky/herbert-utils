<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_character_bow">

	<typeAlias alias="BowTransportData" type="datatransport.bean.characterbow.BowTransportData"/>
	<resultMap class="BowTransportData" id="BowTransportDataRes">
		<result column="f_characterid" jdbcType="INTEGER" property="characterid" />
		<result column="f_bowmodelid" jdbcType="INTEGER" property="bowmodelid" />
		<result column="f_bag1type" jdbcType="INTEGER" property="bag1type" />
		<result column="f_bag1count" jdbcType="INTEGER" property="bag1count" />
		<result column="f_bag2type" jdbcType="INTEGER" property="bag2type" />
		<result column="f_bag2count" jdbcType="INTEGER" property="bag2count" />
		<result column="f_skillitems" jdbcType="VARCHAR" property="skillitems" />
		<result column="f_faildcount" jdbcType="INTEGER" property="faildcount" />
		<result column="f_bag1bind" jdbcType="INTEGER" property="bag1bind" />
		<result column="f_bag2bind" jdbcType="INTEGER" property="bag2bind" />
		<result column="f_enable" jdbcType="BIT" property="enable" />
		<result column="f_luck" jdbcType="INTEGER" property="luck" />
	</resultMap>

	<select id="selectByPrimaryKey" parameterClass="java.lang.Integer" resultMap="BowTransportDataRes">
		select * from t_character_bow where f_characterid = #characterid#
	</select>

	<delete id="deleteByPrimaryKey" parameterClass="java.lang.Integer">
		delete from t_character_bow where f_characterid = #characterid#
	</delete>

	<insert id="insert" parameterClass="BowTransportData">
		insert into t_character_bow (f_characterid, f_bowmodelid, f_bag1type,
		f_bag1count, f_bag2type,
		f_bag2count, f_skillitems, f_faildcount,
		f_bag1bind, f_bag2bind, f_enable, f_luck)
		values (#characterid:INTEGER#, #bowmodelid:INTEGER#,
		#bag1type:INTEGER#, #bag1count:INTEGER#,
		#bag2type:INTEGER#, #bag2count:INTEGER#,
		#skillitems:VARCHAR#, #faildcount:INTEGER#,
		#bag1bind:INTEGER#, #bag2bind:INTEGER#, #enable:BIT#, #luck:INTEGER#)
	</insert>
</sqlMap>