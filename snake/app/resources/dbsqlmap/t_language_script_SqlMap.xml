<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="t_language_script">
	<resultMap id="LanguageScript" class="net.snake.gamemodel.language.bean.LanguageScript">
		<result column="f_id" property="id" jdbcType="INTEGER" />
		<result column="f_key" property="key" jdbcType="VARCHAR" />
		<result column="f_value" property="value" jdbcType="VARCHAR" />
		<result column="f_value_i18n" property="valueI18n" jdbcType="VARCHAR" />
	</resultMap>
	<select id="queryall" resultMap="LanguageScript">
		select f_id, f_key,f_value,f_value_i18n from t_language_script
	</select>
</sqlMap>