<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_character_teamfighting">
	<typeAlias alias="CharacterTeamfighting" type="net.snake.gamemodel.team.bean.CharacterTeamfighting" />
	<resultMap class="CharacterTeamfighting" id="CharacterTeamfightingRes">
		<result column="id" jdbcType="INTEGER" property="id" />
		<result column="character_id" jdbcType="INTEGER" property="characterId" />
		<result column="teamfighting_id" jdbcType="INTEGER" property="teamfightingId" />
		<result column="learn_time" jdbcType="DATE" property="learnTime" />
	</resultMap>

	<select id="selectByCharacterId" resultMap="CharacterTeamfightingRes" parameterClass="java.lang.Integer">
		select id, character_id, teamfighting_id, learn_time from t_character_teamfighting
		where character_id=#characterId#
	</select>

	<insert id="insert" parameterClass="CharacterTeamfighting">
		insert into t_character_teamfighting (id, character_id,
		teamfighting_id, learn_time)
		values (#id:INTEGER#, #characterId:INTEGER#,
		#teamfightingId:INTEGER#,
		#learnTime:DATE#)
	</insert>
</sqlMap>