<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_faction_character">
	<typeAlias alias="FactionCharacter" type="net.snake.gamemodel.faction.bean.FactionCharacter" />
	<resultMap class="FactionCharacter" id="FactionCharacterRes">
		<result column="f_id" jdbcType="INTEGER" property="id" />
		<result column="f_character_id" jdbcType="INTEGER" property="characterId" />
		<result column="f_faction_id" jdbcType="INTEGER" property="factionId" />
		<result column="f_position" jdbcType="TINYINT" property="position" />
		<result column="f_contribution" jdbcType="INTEGER" property="contribution" />
		<result column="f_copper" jdbcType="INTEGER" property="copper" />
		<result column="f_qinglong_count" jdbcType="INTEGER" property="qinglongCount" />
		<result column="f_baihu_count" jdbcType="INTEGER" property="baihuCount" />
		<result column="f_xuanwu_count" jdbcType="INTEGER" property="xuanwuCount" />
		<result column="f_zhuqu_count" jdbcType="INTEGER" property="zhuquCount" />
		<result column="f_name" jdbcType="VARCHAR" property="name" />
		<result column="f_bangzhuling_count" jdbcType="INTEGER" property="bangzhulingCount" />
		<result column="f_tishi_config" jdbcType="INTEGER" property="tishiConfig" />
		<result column="f_date" jdbcType="TIMESTAMP" property="date" />
	</resultMap>

	<select id="queryall" resultMap="FactionCharacterRes">
		select * from t_faction_character
	</select>

	<delete id="deleteByFactionID" parameterClass="java.lang.Integer">
		delete from t_faction_character where f_faction_id = #factionID#
	</delete>

	<delete id="deleteById" parameterClass="java.lang.Integer">
		delete from t_faction_character where f_id = #id#
	</delete>

	<insert id="insert" parameterClass="FactionCharacter">
		insert into t_faction_character (f_character_id, f_faction_id,
		f_position, f_contribution,
		f_copper, f_qinglong_count, f_baihu_count, f_xuanwu_count, f_zhuqu_count,
		f_name,
		f_bangzhuling_count, f_tishi_config, f_date)
		values (#characterId:INTEGER#, #factionId:INTEGER#, #position:TINYINT#,
		#contribution:INTEGER#,
		#copper:INTEGER#,
		#qinglongCount:INTEGER#, #baihuCount:INTEGER#, #xuanwuCount:INTEGER#,
		#zhuquCount:INTEGER#, #name:VARCHAR#, #bangzhulingCount:INTEGER#,
		#tishiConfig:INTEGER#,
		#date:TIMESTAMP#)
		<selectKey keyProperty="id" resultClass="java.lang.Integer">
			SELECT LAST_INSERT_ID()
		</selectKey>
	</insert>

	<update id="updateByPrimaryKeySelective" parameterClass="FactionCharacter">
		update t_faction_character
		<dynamic prepend="set">
			<isNotNull prepend="," property="characterId">
				f_character_id = #characterId:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="factionId">
				f_faction_id = #factionId:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="position">
				f_position = #position:TINYINT#
			</isNotNull>
			<isNotNull prepend="," property="contribution">
				f_contribution = #contribution:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="copper">
				f_copper = #copper:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="qinglongCount">
				f_qinglong_count = #qinglongCount:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="baihuCount">
				f_baihu_count = #baihuCount:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="xuanwuCount">
				f_xuanwu_count = #xuanwuCount:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="zhuquCount">
				f_zhuqu_count = #zhuquCount:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="name">
				f_name = #name:VARCHAR#
			</isNotNull>
			<isNotNull prepend="," property="bangzhulingCount">
				f_bangzhuling_count = #bangzhulingCount:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="tishiConfig">
				f_tishi_config = #tishiConfig:INTEGER#
			</isNotNull>
			<isNotNull prepend="," property="date">
				f_date = #date:TIMESTAMP#
			</isNotNull>
		</dynamic>
		where f_id = #id:INTEGER#
	</update>
</sqlMap>