<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="t_role_random_name">
	<resultMap id="RoleRandomName" class="net.snake.gamemodel.hero.bean.RoleRandomName">
		<result column="f_id" property="fId" jdbcType="INTEGER" />
		<result column="f_type" property="fType" jdbcType="INTEGER" />
		<result column="f_value" property="fValue" jdbcType="VARCHAR" />
	</resultMap>

	<select id="queryall" resultMap="RoleRandomName">
		select f_id, f_type, f_value
		from t_role_random_name
	</select>
</sqlMap>