<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_daypk_record">
	<resultMap class="net.snake.gamemodel.fight.bean.DaypkRecord" id="DaypkRecord">
		<result column="f_winer" jdbcType="INTEGER" property="winer" />
		<result column="f_failer" jdbcType="INTEGER" property="failer" />
		<result column="f_count" jdbcType="INTEGER" property="count" />
		<result column="f_daytime" jdbcType="TIMESTAMP" property="daytime" />
	</resultMap>
	<select id="queryall" parameterClass="java.lang.Integer" resultMap="DaypkRecord">
		select f_winer, f_failer, f_count, f_daytime from t_daypk_record where f_winer=#winerid#
	</select>
</sqlMap>