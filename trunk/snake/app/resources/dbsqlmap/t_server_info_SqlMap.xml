<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_server_info">
	<typeAlias alias="Severinfo" type="net.snake.gamemodel.line.bean.Severinfo" />
	<resultMap class="Severinfo" id="SeverinfoRes">
		<result column="f_id" jdbcType="INTEGER" property="id" />
		<result column="f_kaifu_time" jdbcType="TIMESTAMP" property="kaifuTime" />
	</resultMap>
	<select id="queryById" resultMap="SeverinfoRes" parameterClass="java.lang.Integer">
		select f_id, f_kaifu_time from t_server_info where f_id=#id#
	</select>

	<insert id="insert" parameterClass="Severinfo">
		insert into t_server_info
		(f_id, f_kaifu_time) values (#id:INTEGER#, #kaifuTime:TIMESTAMP#)
	</insert>
</sqlMap>