<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_shop">
	<resultMap class="net.snake.gamemodel.shop.bean.Shop" id="Shop">
		<result column="f_id" jdbcType="INTEGER" property="id" />
		<result column="f_goods_type" jdbcType="INTEGER" property="goodsType" />
		<result column="f_name" jdbcType="VARCHAR" property="name" />
		<result column="f_goodmodel_id" jdbcType="INTEGER" property="goodmodelId" />
		<result column="f_sale_type" jdbcType="INTEGER" property="saleType" />
		<result column="f_goods_price" jdbcType="INTEGER" property="goodsPrice" />
		<result column="f_hot_mark" jdbcType="INTEGER" property="hotMark" />
		<result column="f_abate_mark" jdbcType="INTEGER" property="abateMark" />
		<result column="f_order" jdbcType="INTEGER" property="order" />
		<result column="f_goods_old_price" jdbcType="INTEGER" property="goodsOldPrice" />
		<result column="f_isdel" jdbcType="INTEGER" property="isdel" />
		<result column="f_view_time" jdbcType="VARCHAR" property="viewTime" />
		<result column="f_limite_count" jdbcType="INTEGER" property="limiteCount" />
		<result column="f_is_bind" jdbcType="INTEGER" property="isBind" />
		<result column="f_last_date" jdbcType="VARCHAR" property="lastDate" />
	</resultMap>

	<select id="queryall" resultMap="Shop">
		select * from t_shop order by f_order
	</select>
</sqlMap>