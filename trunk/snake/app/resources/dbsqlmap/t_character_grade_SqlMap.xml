<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_character_grade">
	<typeAlias alias="Charactergrade" type="net.snake.gamemodel.hero.bean.Charactergrade" />
	<resultMap class="Charactergrade" id="CharactergradeRes">
		<result column="f_grade" jdbcType="SMALLINT" property="grade" />
		<result column="f_hp_add" jdbcType="INTEGER" property="hpAdd" />
		<result column="f_mp_add" jdbcType="INTEGER" property="mpAdd" />
		<result column="f_sp_add" jdbcType="INTEGER" property="spAdd" />
		<result column="f_hit_add" jdbcType="INTEGER" property="hitAdd" />
		<result column="f_potential_add" jdbcType="INTEGER" property="potentialAdd" />
		<result column="f_attack_add" jdbcType="INTEGER" property="attackAdd" />
		<result column="f_def_add" jdbcType="INTEGER" property="defAdd" />
		<result column="f_expose_add" jdbcType="INTEGER" property="exposeAdd" />
		<result column="f_dodge_add" jdbcType="INTEGER" property="dodgeAdd" />
		<result column="f_exp_add" jdbcType="INTEGER" property="expAdd" />
		<result column="f_atk_speed" jdbcType="INTEGER" property="atkSpeed" />
		<result column="f_move_speed" jdbcType="INTEGER" property="moveSpeed" />
		<result column="f_popsinger" jdbcType="INTEGER" property="popsinger" />
		<result column="f_key" jdbcType="INTEGER" property="key" />
	</resultMap>
	<select id="queryall" resultMap="CharactergradeRes">
		select a.*,concat(f_popsinger,'_',f_grade) as f_key from t_character_grade as a
	</select>
</sqlMap>