<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>
	<typeAlias alias="CharacterBuff" type="net.snake.gamemodel.skill.bean.BuffPersisData" />

	<resultMap id="CharacterBuffRes" class="CharacterBuff">
		<result property="characterid" column="f_characterId" />
		<result property="duration" column="f_duration" />
		<result property="buffvalue" column="f_buffValue" />
		<result property="relevanceSkillid" column="f_relevance_skillId" />
		<result property="gotime" column="f_gotime" />
		<result property="effectId" column="f_effect_id" />
		<result property="duration2" column="f_duration2" />
		<result property="remainpoint" column="f_remainpoint" />
		<result property="delayrecoverTime" column="f_delayrecover_time" />
		<result property="effectInfoType" column="f_type" />
		<result property="jingmaiId" column="f_jingmai_id" />
	</resultMap>

	<select id="characterBuffGetAllByCharacterid" parameterClass="java.lang.Integer" resultMap="CharacterBuffRes">
		select * from t_character_buff where f_characterId=#characterid#
	</select>

</sqlMap>
