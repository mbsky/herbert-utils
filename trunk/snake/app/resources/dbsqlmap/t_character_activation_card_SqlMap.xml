<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="t_character_activation_card">
	<typeAlias alias="CharacterActivationCard" type="net.snake.gamemodel.activities.bean.CharacterActivationCard" />
	<resultMap id="CharacterActivationCardRes" class="CharacterActivationCard">
		<result column="f_id" property="id" jdbcType="INTEGER" />
		<result column="f_card_no" property="cardNo" jdbcType="VARCHAR" />
		<result column="f_card_type" property="cardType" jdbcType="TINYINT" />
		<result column="f_sid" property="sid" jdbcType="INTEGER" />
		<result column="f_account_id" property="accountId" jdbcType="INTEGER" />
		<result column="f_character_id" property="characterId" jdbcType="INTEGER" />
		<result column="f_time" property="time" jdbcType="TIMESTAMP" />
		<result column="f_ip" property="ip" jdbcType="VARCHAR" />
		<result column="f_character_grade" property="characterGrade" jdbcType="INTEGER" />
	</resultMap>

	<select id="queryall" resultMap="CharacterActivationCardRes">
		select * from t_character_activation_card
	</select>

	<insert id="insert" parameterClass="CharacterActivationCard">
		insert into t_character_activation_card (f_card_no, f_card_type,
		f_sid, f_account_id,
		f_character_id, f_time, f_ip, f_character_grade)
		values (#cardNo:VARCHAR#, #cardType:TINYINT#,
		#sid:INTEGER#,
		#accountId:INTEGER#,
		#characterId:INTEGER#, #time:TIMESTAMP#, #ip:VARCHAR#, #characterGrade:INTEGER#)
		<selectKey resultClass="java.lang.Integer" keyProperty="id">
			SELECT LAST_INSERT_ID()
		</selectKey>
	</insert>
</sqlMap>