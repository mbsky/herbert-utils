<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="t_doub_activity">
	<resultMap class="net.snake.gamemodel.activities.bean.DoubActivity" id="DoubActivity">
		<result column="f_sid" jdbcType="INTEGER" property="sid" />
		<result column="f_daguai_exp" jdbcType="INTEGER" property="daguaiExp" />
		<result column="f_dazuo_exp" jdbcType="INTEGER" property="dazuoExp" />
		<result column="f_baolv" jdbcType="INTEGER" property="baolv" />
		<result column="f_dazuo_zhenqi" jdbcType="INTEGER" property="dazuoZhenqi" />
		<result column="f_time_exp" jdbcType="VARCHAR" property="timeExp" />
	</resultMap>

	<select id="selectByPrimaryKey" parameterClass="java.lang.Integer" resultMap="DoubActivity">
		select f_sid, f_daguai_exp, f_dazuo_exp, f_baolv,
		f_dazuo_zhenqi,f_time_exp
		from t_doub_activity
		where f_sid = #sid#
	</select>

</sqlMap>